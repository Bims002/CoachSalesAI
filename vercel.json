{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/vite",
      "config": { "project": "./package.json" }
    },
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/index.js"
    }
  ]
}
